class Car {
    constructor(year,make,model) {
        this.year = year;
        this.make = make;
        this.model = model;
    }

    print() {
        console.log(`
            Year: ${this.year}
            Make: ${this.model}
            Model: ${this.model}
        `);
    }
}

var dailyDriver = new Car(
    2000,
    "Toyota",
    "Camry"
);

dailyDriver.print();